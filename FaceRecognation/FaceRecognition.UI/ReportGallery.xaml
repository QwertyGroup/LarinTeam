<Window x:Class="FaceRecognition.UI.Galley.ReportGallery"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FaceRecognition.UI.Galley"
        mc:Ignorable="d"
        Title="Report Gallery" Height="400" Width="600">
	<Grid>
		<TabControl x:Name="tabControl" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
			<TabItem Header="New people">
				<StackPanel x:Name="spNew" Background="#FFE5E5E5"/>
			</TabItem>
			<TabItem Header="Already existed">
				<StackPanel x:Name="spExisted" Background="#FFE5E5E5"/>
			</TabItem>
		</TabControl>

	</Grid>
</Window>
